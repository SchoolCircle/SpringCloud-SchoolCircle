<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.chat.dao.UserDao">
    <select id="findByUid" resultType="com.example.chat.entity.User" parameterType="integer">
        select uid,email,token from school_circle.user where uid=#{uid};
    </select>
</mapper>